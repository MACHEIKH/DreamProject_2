                                    "Fixed",
                                    "Exp",
                                    "Normal",
                                    "Lognormal",
                                    "Binomial",
                                    "Poisson",
                                    "Logistic",
                                    "Cauchy",
                                    "Geometric",
                                    "Gama",
                                    "Weibull"
                                ],
                                "type": "string"
                            }
                        },
                        "type": "object"
                    },
                    {
                        "oneOf": [
                            {
                                "$ref": "#/definitions/distributionTypes/_fixed"
                            },
