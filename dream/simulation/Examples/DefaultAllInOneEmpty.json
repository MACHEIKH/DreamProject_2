                        "properties": {
                            "failureDistribution": {
                                "default": "No",
                                "enum": [
                                    "No",
                                    "Yes"
                                ],
                                "type": "string"
                            }
                        },
                        "type": "object"
                    },
                    {
                        "oneOf": [
                            {
                                "$ref": "#/definitions/distributionTypes/_failure"
